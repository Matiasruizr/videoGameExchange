<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Nombre usuario</th>
      <th>Contrasena</th>
      <th>Rut</th>
      <th>Direccion</th>
      <th>Sexo</th>
      <th>Celular</th>
      <th>Codigo postal</th>
      <th>Fecha nacimiento</th>
      <th>Id comuna</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.id %></td>
        <td><%= usuario.nombre %></td>
        <td><%= usuario.apellido %></td>
        <td><%= usuario.nombre_usuario %></td>
        <td><%= usuario.contrasena %></td>
        <td><%= usuario.rut %></td>
        <td><%= usuario.direccion %></td>
        <td><%= usuario.sexo %></td>
        <td><%= usuario.celular %></td>
        <td><%= usuario.codigo_postal %></td>
        <td><%= usuario.fecha_nacimiento %></td>
        <td><%= usuario.id_comuna %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
